    "# Fibonacci numbers module\n",
    "\n",
    "def fib(n):\n",
    "    a, b = 0,1\n",
    "    while a<n:\n",
    "        print(a,end = ' ')\n",
    "        a, b = b, a+b\n",
    "    print()\n",
    "\n",
    "def fib2(n):\n",
    "    result = []\n",
    "    a,b = 0,1\n",
    "    while a<n:\n",
    "        result.append(a)\n",
    "        a, b = b, a+b\n",
    "    return result"
   ]
